<?xml version="1.0"?>
	<run>
	    <template>
	        <about>This is a Nautilus template for the sod shock</about>
	    </template>

	        <number id="pLeft">
	            <about>
	                <label>pressure on the left half of the domain</label>
	                <description>Pressure dude!</description>
	            </about>
	            <units>pascals</units>
	            <min>0.0</min>
	            <default>1.0e5</default>
            </number>
            
	        <number id="pRight">
	            <about>
	                <label>pressure on the right half of the domain</label>
	                <description>Pressure second dude!</description>
	            </about>
	            <units>pascals</units>
	            <min>0.0</min>
	            <default>1.0e5</default>
	        </number>

	        <number id="rhoLeft">
	            <about>
	                <label>density on the left half of the domain</label>
	                <description>density dude!</description>
	            </about>
	            <units> kg/m^3 </units>
	            <min>0.0</min>
	            <default>1.0</default>
            </number>
            
	        <number id="rhoRight">
	            <about>
	                <label>density on the right half of the domain</label>
	                <description>density second dude!</description>
	            </about>
	            <units>kg/m^3</units>
	            <min>0.0</min>
	            <default>0.1</default>
	        </number>

	</run>
